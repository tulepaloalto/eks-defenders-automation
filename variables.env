####################################################################################################
# Configure for Code Logic
####################################################################################################
DEBUG_MODE=True
# Print API calls and only send read-only API requests

GENERATE_LOG_FILES=False
# True/False
#   True to generate a log file locally or False to log to STDOUT.

LOG_FILE_LIMIT=1
# int - Maximum number of log files to generate, automation will delete the oldest logs.

####################################################################################################
# Configure for Prisma
####################################################################################################
CSPM_ENDPOINT="api.prismacloud.io"
# Cloud Security Posture Management API Endpoint

ACCESS_KEY_EXPIRATION=180
# int - Number of days before an access key expires and new key is created.

ACCESS_KEY_BUFFER=7
# int - Number of days until removal of the old access key after expiration.

ACCESS_KEY_FILTER=[]
# If populated, will only rotate the keys defined. (e.g.) [] or ["key1", "key2", "key3"]

####################################################################################################
# Configure for AWS
####################################################################################################

AWS_AUTOMATION_SECRET_NAME="Prisma-Automation-Secrets"
# The name of the Secret containing the Prisma Keys for the automation itself.

AWS_REGION="us-east-1"
# The region of the Secrets Manager

